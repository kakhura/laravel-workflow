{
    "name": "kakhura/laravel-workflow",
    "description": "Integerate Symfony Workflow component into Laravel.",
    "keywords": [
        "workflow",
        "symfony",
        "laravel",
        "laravel5",
        "laravel6"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/workflow": "^3.3 || ^4.0 || ^5.0 || ^6.3 || ^7.2",
        "symfony/process": "^3.3 || ^4.0 || ^5.0 || ^6.3 || ^7.2",
        "symfony/event-dispatcher": "^3.3 || ^4.0 || ^5.0 || ^6.3 || ^7.2",
        "illuminate/console": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.* || 10.* || 11.*",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.* || 10.* || 11.*"
    },
    "autoload": {
        "psr-4": {
            "Brexis\\LaravelWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Brexis\\LaravelWorkflow\\WorkflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "Brexis\\LaravelWorkflow\\Facades\\WorkflowFacade"
            }
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0 || ~7.0 || ^8.0"
    }
}
